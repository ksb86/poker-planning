{
  "name": "poker-planning",
  "version": "1.0.0",
  "scripts": {
    "dev": "concurrently 'yarn dev:server' 'yarn dev:client'",
    "dev:server": "nodemon ./server/server.js",
    "dev:client": "webpack -w --config webpack.dev",
    "build:client": "webpack -p --config webpack.prod",
    "copyAssets": "cp ./client/assets/* ./dist/",
    "prod": "yarn && yarn build:client && yarn copyAssets && yarn restart",
    "restart": "pm2 stop poker-planning; pm2 start --name poker-planning server/server.js"
  },
  "dependencies": {
    "@use-it/interval": "^0.1.3",
    "body-parser": "^1.18.3",
    "chart.js": "^2.8.0",
    "classnames": "^2.2.6",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.4",
    "core-js": "^3.2.1",
    "dotenv": "^10.0.0",
    "express": "^4.17.3",
    "glob": "^7.1.5",
    "konami": "^1.6.2",
    "less": "^3.10.3",
    "react": "^16.12.0",
    "react-chartjs-2": "^2.8.0",
    "react-dom": "^16.12.0",
    "react-icons": "^4.2.0",
    "react-redux": "^7.1.3",
    "redux": "^4.0.4",
    "redux-devtools-extension": "^2.13.8",
    "redux-promise-middleware": "^6.1.2",
    "redux-thunk": "^2.3.0",
    "shortid": "^2.2.15",
    "solitairewin": "^0.0.12"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-decorators": "^7.6.0",
    "@babel/plugin-proposal-optional-chaining": "^7.7.5",
    "@babel/preset-env": "^7.6.2",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.6.2",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^4.1.2",
    "css-loader": "^3.2.0",
    "eslint": "^6.5.1",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.16.0",
    "html-webpack-plugin": "^3.2.0",
    "less-loader": "^5.0.0",
    "mini-css-extract-plugin": "^0.8.0",
    "nodemon": "^1.19.3",
    "pm2": "^3.2.8",
    "webpack": "^4.41.0",
    "webpack-cli": "^3.3.9"
  },
  "private": true
}
